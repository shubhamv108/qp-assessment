[SERVICE]
    Flush        5
    Daemon       Off
    Log_Level    info
    Parsers_File parsers.conf

[INPUT]
    Name forward
    Listen 0.0.0.0
    port 24224

[OUTPUT]
    Name  stdout
    Match *

[OUTPUT]
    Name cloudwatch_logs
    Match *
    region ap-south-1
    log_group_name grocery-booking
    log_stream_prefix grocery-booking-
    auto_create_group On
